{
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "define": true,
    "require": true,
    "requirejs": true,
    "$": true,
    "_": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "quotes": [2, "single"],
    "brace-style": [2, "1tbs"],
    "no-multi-spaces": 2,
    "no-trailing-spaces": 2,
    "space-after-keywords": [2, "always"],
    "space-before-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "indent": [2, 4, {"SwitchCase": 1}],
    "semi": [2, "always"],
    "no-unused-vars":0,
    "no-console":0,
    "no-undef":0
  }
}